{
  "manifest_version": 2,
  "name": "Chat Encryption",
  "permissions": [
    "storage",
    "tabs",
    "https://meet.google.com/*",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["content.js"]
    }
  ]
}
